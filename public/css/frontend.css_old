.input_checkbox, .input_radio, .input_select_room {
    -webkit-appearance: none;
    -moz-appearance: none;
    -ms-appearance: none;
    -o-appearance: none;
    appearance: none;
    position: relative;
    top: 5px;
    right: 0;
    bottom: 0;
    left: 0;
    height: 20px;
    width: 20px;
    -webkit-transition: all 0.15s ease-out 0s;
    -moz-transition: all 0.15s ease-out 0s;
    transition: all 0.15s ease-out 0s;
    background: #fff;
    border: 1px solid #333;
    color: #333;
    cursor: pointer;
    display: inline-block;
    margin-right: 0.5rem!important;;
    outline: 0 !important;
    z-index: 1000;
    float: left;
}

.input_select_room, .input_radio {
    border-radius: 10px;
}

.input_checkbox:checked:before {
    height: 20px;
    width: 20px;
    position: absolute;
    content: '\2715';
    display: inline-block;
    font-size: 20px;
    text-align: center;
    margin-top: -4.9px;
    margin-left: -0.5px;
}

.input_select_room:checked:before, .input_radio:checked:before {
    height: 12px;
    width: 12px;
    position: absolute;
    content: '';
    display: inline-block;
    text-align: center;
    margin-top: 2.5px;
    margin-left: 2.7px;
    line-height: 10px;
    background-color: #000;
    border-radius: 10px;
}

