.action_container.active {
    /* Permalink - use to edit and share this gradient: http://colorzilla.com/gradient-editor/#efefef+0,eaeaea+72,ffffff+94,ededed+100,ededed+100,e2e2e2+101 */
    background: #efefef; /* Old browsers */
    background: -moz-linear-gradient(left,  #efefef 0%, #eaeaea 72%, #ffffff 94%, #ededed 100%, #ededed 100%, #e2e2e2 101%); /* FF3.6-15 */
    background: -webkit-linear-gradient(left,  #efefef 0%,#eaeaea 72%,#ffffff 94%,#ededed 100%,#ededed 100%,#e2e2e2 101%); /* Chrome10-25,Safari5.1-6 */
    background: linear-gradient(to right,  #efefef 0%,#eaeaea 72%,#ffffff 94%,#ededed 100%,#ededed 100%,#e2e2e2 101%); /* W3C, IE10+, FF16+, Chrome26+, Opera12+, Safari7+ */
    filter: progid:DXImageTransform.Microsoft.gradient( startColorstr='#efefef', endColorstr='#e2e2e2',GradientType=1 ); /* IE6-9 */
    padding: 10px 0;
    margin-left: -25px;
    margin-right: -25px;
}

